<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>datatables.net-se</id>
    <version>1.10.24</version>
    <description>SemanticUI integration for DataTables</description>
    <authors>SpryMedia Ltd</authors>
    <projectUrl>http://datatables.net</projectUrl>
    <license type="expression">MIT</license>
    <tags> DataTables jQuery table SemanticUI</tags>
    <repository type="git" url="https://github.com/DataTables/Dist-DataTables-SemanticUI.git"/>
    <dependencies>
      <dependency id="jquery" version="1.7.0"/>
      <dependency id="datatables.net" version="1.10.23"/>
      <dependency id="semanticui" version="2.*"/>
    </dependencies>
    <contentFiles>
      <files include="**/*" buildAction="Content"/>
    </contentFiles>
  </metadata>
  <files>
    <file src="js/dataTables.semanticui.js" target="content\Scripts"/>
    <file src="js/dataTables.semanticui.min.js" target="content\Scripts"/>
    <file src="css/dataTables.semanticui.css" target="content\Content"/>
    <file src="css/dataTables.semanticui.min.css" target="content\Content"/>
    <file src="js/dataTables.semanticui.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="js/dataTables.semanticui.min.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="css/dataTables.semanticui.css" target="contentFiles\any\any\wwwroot\css"/>
    <file src="css/dataTables.semanticui.min.css" target="contentFiles\any\any\wwwroot\css"/>
  </files>
</package>
